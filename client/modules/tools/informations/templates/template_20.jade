table
  tr(ng-if='data.is_in_place')
    td {{ 'informations.situation' | translate }}
    td
      a(ng-click='onClick(data.is_in_place[0].id)') {{ data.is_in_place[0].name }}
  tr(ng-if='data.address')
    td {{ 'informations.home' | translate }}
    td
      span(ng-repeat='item in data.address')
        a(ng-click='onClick(item.id)') {{ item.name }}
        span(ng-if='!$last') , 
  tr(ng-if='data.type')
    td {{ 'informations.type' | translate }}
    td {{ data.type }}
